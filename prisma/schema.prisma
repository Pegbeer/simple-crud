model OrderHeader {
  Id                                         String           @id(map: "PK__OrderHea__3214EC078AAADF2E") @default(dbgenerated("newid()"), map: "DF__OrderHeader__Id__6383C8BA") @db.UniqueIdentifier
  SubTotal                                   Decimal          @db.Decimal(10, 2)
